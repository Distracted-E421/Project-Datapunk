{
  "datacenter": "dc1",
  "data_dir": "/consul/data",
  "log_level": "INFO",
  "node_name": "consul-server",
  "server": true,
  "bootstrap_expect": 1,
  "ui_config": {
    "enabled": true
  },
  "connect": {
    "enabled": true
  },
  "ports": {
    "dns": 8600,
    "http": 8500,
    "https": -1,
    "grpc": 8502
  },
  "telemetry": {
    "prometheus_retention_time": "24h",
    "disable_hostname": true
  }
} 